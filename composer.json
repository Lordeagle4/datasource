{
    "name": "awtechs/datasource",
    "description": "A Laravel repository pattern implementation for Eloquent models with caching and query utilities",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Awtechs\\Datasource\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Michael Ugochukwu",
            "email": "eaglemike7@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/database": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.5"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Awtechs\\Datasource\\Providers\\DatasourceServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/awtechs"
        }
    ]
}